{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "src",
    "paths": {
      "buildings": ["domain/buildings"],
      "buildings/*": ["domain/buildings/*"],
      "dto": ["domain/dto"],
      "mock": ["domain/mock"],
      "enum": ["domain/enum"],
      "type": ["domain/type"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/hooks/useMissiles.ts"],
  "exclude": ["node_modules"]
}
